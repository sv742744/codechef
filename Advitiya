import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s=new Scanner(System.in);
		int X=s.nextInt();
		if(X>=16)
		    System.out.println("ADVITIYA");
		else    
		    System.out.println("WAITING FOR ADVITIYA");
	}
}
