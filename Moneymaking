import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner s= new Scanner(System.in);
		int X=s.nextInt();
		int Y=s.nextInt();
		System.out.println((X*5000)+(Y*9800));
		
	}
}
